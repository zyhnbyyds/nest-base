{
  "name": "nest-base",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build core & nest build auth & nest build logger & nest build email",
    "dev:core": "cross-env NODE_ENV=dev nest start core -w",
    "dev:auth": "cross-env NODE_ENV=dev nest start auth -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:init": "prisma migrate dev --name init",
    "compodoc": "compodoc -p tsconfig.doc.json -d apps-doc -r 3301 -s --language zh-CN",
    "compodoc:init": "compodoc -p tsconfig.doc.json -d apps-doc -r 3301 --language zh-CN",
    "run:auth": "cross-env NODE_ENV=dev npm-run-all --parallel dev:auth micro:email micro:logger",
    "run:core": "cross-env NODE_ENV=dev npm-run-all --parallel dev:core micro:logger",
    "run:micro": "cross-env NODE_ENV=dev npm-run-all -p micro:email micro:logger",
    "run:all": "cross-env NODE_ENV=dev npm-run-all -p run:micro dev:core dev:auth",
    "micro:logger": "nest start logger --watch",
    "micro:email": "nest start email --watch"
  },
  "dependencies": {
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/one-line-logger": "^2.0.0",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/static": "^8.0.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.4",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.4.4",
    "@nestjs/platform-fastify": "^10.4.4",
    "@nestjs/platform-socket.io": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.4.4",
    "@prisma/client": "^5.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "ms": "3.0.0-canary.1",
    "nodemailer": "^6.9.15",
    "openapi3-ts": "^4.4.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
