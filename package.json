    "@types/node": "^22.7.5",
    "@types/nodemailer": "^6.4.16",
    "eslint": "^9.12.0",
    "fastify": "^4.28.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
